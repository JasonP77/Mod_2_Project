
<h1><%= @user.username %> edit form</h1>
<br>
<br>
<%= form_for @user do |f| %>
    <%= f.label :full_name %>
    <%= f.text_field :full_name %>
    <br>
    <br>
    <%= f.label :username %>
    <%= f.text_field :username %>
    <br>
    <br>
    <%= f.label :email %>
    <%= f.text_field :email %>
    <br>
    <%= f.submit %>
<% end %>

<%link_to "Users Page", users_path %>